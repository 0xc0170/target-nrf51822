{
  "name": "nordic-nrf51822-32k-armcc",
  "version": "0.0.5",
  "description": "Official mbed build target for the mbed nrf51822 development board, using the armcc toolchain.",
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    }
  ],
  "inherits": {
    "mbed-armcc": "*"
  },
  "keywords": [
    "mbed-target:nrf51822",
    "mbed-official",
    "nrf51822",
    "nordic",
    "armcc"
  ],
  "config": {
    "minar": {
      "initial_event_pool_size": 4,
      "additional_event_pools_size": 4
    }
  },
  "similarTo": [
    "nrf51822",
    "nrf",
    "nordic",
    "cortex-m0",
    "armv6-m"
  ],
  "toolchain": "CMake/toolchain.cmake",
  "scripts": {
    "debug": [
      "valinor",
      "--target",
      "NRF51822",
      "$program"
    ],
    "test": [
      "mbed_test_wrapper",
      "--target",
      "NRF51822",
      "$program"
    ]
  }
}